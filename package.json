{
  "name": "agrolink-monorepo",
  "private": true,
  "workspaces": [
    "apps/backend",
    "apps/frontend"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:backend": "docker compose up",
    "dev:frontend": "cd apps/frontend && npm run dev",
    "dev:apps": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "turbo": "^1.13.4"
  },
  "dependencies": {
    "react-router-hash-link": "^2.4.3"
  }
}
